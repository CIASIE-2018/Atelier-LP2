{{> header}}
    <h1 class="infoCoffrets">
    Coffret 
    {{#box.isCurrent}} courant {{/box.isCurrent}}
    {{^box.isCurrent}} non courant {{/box.isCurrent}}
    contenant {{box.prestations.length}} prestations.
    </h1>
    <div class="container">
        {{#box.prestations}}
        <div class="prestation">
            <h1 class="titrePrestation">{{title}}</h1>
            <div class="card-img">
                <img src="/img/{{image}}">
            </div>
            <div>{{description}}</div>
            <span>Prix: {{price}}€</span>
        </div>
        {{/box.prestations}}
    </div>
    {{^isCurrent}}
        <a href="/changeCurr/{{box._id}}" class="modifsBox">Sélectionner le coffret comme coffret courant</a>
    {{/isCurrent}}
        <a href="/delete/{{box._id}}" class="modifsBox">Supprimer le coffret</a>
    
{{> footer}}